<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    android:paddingHorizontal="20dp"    android:paddingVertical="28dp"    android:background="@color/white"    tools:context=".ui.screen.settings.SettingsFragment">    <Toolbar        android:id="@+id/titlePushNotifications"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:navigationIcon="@drawable/ic_arrow_left"        android:textStyle="bold"        android:title="@string/notifications"        android:titleTextColor="@color/black" />    <View        android:layout_width="wrap_content"        android:layout_height="32dp" />    <LinearLayout        android:id="@+id/user_info_container"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="start|center"        android:orientation="horizontal"        android:visibility="gone">    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center"        android:orientation="vertical">        <View            android:id="@+id/btn_personal_info_under_line"            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60"            android:visibility="gone" />        <com.google.android.material.switchmaterial.SwitchMaterial            android:id="@+id/btn_faq"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:drawablePadding="8dp"            android:paddingVertical="16dp"            android:text="@string/push_notifications"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <com.google.android.material.switchmaterial.SwitchMaterial            android:id="@+id/btn_faq1"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:drawablePadding="8dp"            android:paddingVertical="16dp"            android:text="@string/sms_notifications"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />    </LinearLayout></LinearLayout>