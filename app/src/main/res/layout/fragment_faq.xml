<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    android:background="@color/white"    android:paddingHorizontal="20dp"    tools:context=".ui.screen.faq.FAQFragment">    <Toolbar        android:layout_width="match_parent"        android:id="@+id/faq_title"        android:layout_height="wrap_content"        android:title="@string/faq"        android:titleTextColor="@color/black"        android:layout_marginTop="28dp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        android:navigationIcon="@drawable/ic_arrow_left"        android:textStyle="bold" />    <LinearLayout        android:id="@+id/faqLayout"        android:layout_width="320dp"        android:layout_height="wrap_content"        android:animateLayoutChanges="true"        android:longClickable="false"        android:weightSum="16"        android:gravity="center"        android:layout_marginTop="32dp"        android:layout_marginStart="20dp"        android:layout_marginEnd="20dp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/faq_title"        android:orientation="vertical">        <Button            android:id="@+id/btn_faq1"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:drawableEnd="@drawable/ic_arrow_down"            android:drawablePadding="8dp"            android:paddingVertical="16dp"            android:text="@string/how_to_register_profile"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textDirection="anyRtl"            android:textSize="15sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60"            />        <TextView            android:id="@+id/tv_faq_answers1"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/faq_answers"            android:layout_gravity="start"            android:visibility="gone"            android:layout_marginTop="8dp"            android:textColor="@color/black"            />        <Button            android:id="@+id/btn_faq2"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:drawableEnd="@drawable/ic_arrow_down"            android:drawablePadding="8dp"            android:paddingVertical="16dp"            android:text="@string/how_to_use_promocode"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp"            android:titleMarginStart="10sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <TextView            android:id="@+id/tv_faq_answers2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/faq_answers_option"            android:layout_gravity="start"            android:layout_marginTop="8dp"            android:visibility="gone"            android:textAlignment="viewStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp"            />        <Button            android:id="@+id/btn_faq3"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:drawableEnd="@drawable/ic_arrow_down"            android:paddingVertical="16dp"            android:text="@string/how_to_use_promocode"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <TextView            android:id="@+id/tv_faq_answers3"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/faq_answers_option"            android:layout_gravity="start"            android:layout_marginTop="8dp"            android:visibility="gone"            android:textAlignment="viewStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp"            />        <Button            android:id="@+id/btn_faq4"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:paddingVertical="16dp"            android:drawableEnd="@drawable/ic_arrow_down"            android:text="@string/how_to_use_promocode"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <TextView            android:id="@+id/tv_faq_answers4"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/faq_answers_option"            android:layout_gravity="start"            android:layout_marginTop="8dp"            android:visibility="gone"            android:textAlignment="viewStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp"            />        <Button            android:id="@+id/btn_faq5"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@android:color/transparent"            android:drawableEnd="@drawable/ic_arrow_down"            android:paddingVertical="16dp"            android:text="@string/how_to_use_promocode"            android:textAlignment="textStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp" />        <View            android:layout_width="wrap_content"            android:layout_height="1dp"            android:background="@color/neutral_n60" />        <TextView            android:id="@+id/tv_faq_answers5"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/faq_answers_option"            android:layout_gravity="start"            android:layout_marginTop="8dp"            android:visibility="gone"            android:textAlignment="viewStart"            android:textAllCaps="false"            android:textColor="@color/black"            android:textSize="15sp"            />    </LinearLayout></androidx.constraintlayout.widget.ConstraintLayout>