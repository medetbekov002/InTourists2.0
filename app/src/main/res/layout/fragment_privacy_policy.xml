<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/fragment_privacy_policy"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:layout_gravity="top"    android:orientation="vertical"    android:background="@color/white"    android:paddingVertical="24dp"    tools:context=".presentation.ui.screen.privacy_policy.PrivacyPolicyFragment">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical"        android:paddingHorizontal="20dp"        tools:ignore="UselessParent">        <Toolbar            android:id="@+id/title_privacy_policy"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginStart="-20dp"            android:navigationIcon="@drawable/ic_arrow_left"            app:layout_scrollFlags="scroll|enterAlways">            <TextView                android:layout_width="match_parent"                android:layout_height="match_parent"                android:text="@string/privacy_policy_title"                android:textColor="@color/black"                android:textSize="21sp"                android:textStyle="bold" />        </Toolbar>        <View            android:layout_width="match_parent"            android:layout_height="32dp" />        <androidx.core.widget.NestedScrollView            android:id="@+id/scroll_tv_privacy_description"            android:layout_width="match_parent"            android:layout_height="match_parent"            app:layout_behavior="@string/appbar_scrolling_view_behavior">            <TextView                android:id="@+id/tv_privacy_description"                android:layout_width="320dp"                android:layout_height="wrap_content"                android:layout_marginStart="20dp"                android:lineSpacingExtra="1sp"                android:text="@string/privacy_policy_description"                android:textColor="@color/black"                android:textSize="15dp"                tools:ignore="SpUsage"                tools:targetApi="p" />        </androidx.core.widget.NestedScrollView>    </LinearLayout></LinearLayout>